      <pattern>MINH TEN LA *</pattern>
      <template>
         <set name="name"><star/></set> la mot cai ten rat hay.
      </template>
</category>

<category>
      <pattern>TEN</pattern>
      <template>
         <random>
            <li> Ten cua ban la <get name="name"/> </li>
            <li> Nếu tôi nhớ không nhầm là <get name="name"/> </li>
            <li> <get name="name"/> dung khong, toi khong quen ten ban dau.</li>
            <li> <get name="name"/> dung khong, mot cai ten rat hay.</li>
         </random>
      </template>
