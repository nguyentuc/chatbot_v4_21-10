<!-- that: using for conversation in one conversation -->
<category>
      <pattern>BAN CO THICH XEM PHIM KHONG</pattern>
      <template>TOI CO,BAN CO THICH XEM PHIM GI KHONG</template>
</category>

<category>
      <pattern>CO</pattern>
      <that>DO YOU LIKE COMEDY MOVIES</that>
      <template>Nice, I like comedy movies too.</template>
</category>

<category>
      <pattern>NO</pattern>
      <that>DO YOU LIKE COMEDY MOVIES</that>
      <template>Ok, but i like comedy movie.</template>
</category>


<!-- topic: set pattern and templates for each topic. -->
<category>
      <pattern>LET DISCUSS MOVIES</pattern>
      <template>Yes <set name = "topic">movies</set></template>
</category>

<topic name = "movies">
      <category>
         <pattern> * </pattern>
         <template>Watching good movie refreshes our minds.</template>
      </category>

      <category>
         <pattern>I LIKE WATCHING COMEDY</pattern>
         <template>I like comedy movies too.</template>
      </category>
</topic>

<!-- think: set name and use after that -->
<category>
      <pattern>NAME *</pattern>
      <template>
         Hello!
         <think>
            <set name = "username"> <star/> </set>
         </think>
      </template>
</category>

<category>
      <pattern>TAM BIET</pattern>
      <template>
         Hi <get name = "username"/> Xin chao va hen gap lai ban lan sau!
      </template>
</category>

<category>
      <pattern>TOI LA AI</pattern>
